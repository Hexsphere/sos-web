{
  "name": "solid-object-software",
  "version": "1.0.0",
  "description": "The official landing page for the company Solid Object Software",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/SolidObjectSoftware/solidobjectsoftware.gitlab.io.git"
  },
  "author": "Ethan Anderson",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/SolidObjectSoftware/solidobjectsoftware.gitlab.io/issues"
  },
  "homepage": "https://gitlab.com/SolidObjectSoftware/solidobjectsoftware.gitlab.io#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1"
  }
}
